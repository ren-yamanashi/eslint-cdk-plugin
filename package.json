{
  "name": "@nigg/eslint-plugin-cdk",
  "version": "0.1.0",
  "description": "eslint plugin for AWS CDK projects",
  "main": "dist/index.mjs",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && tsc src/*.*ts --outDir dist --skipLibCheck",
    "test": "vitest --run",
    "lint": "eslint --fix --config eslint.config.mjs"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/estree": "^1.0.6",
    "@types/node": "^22.9.0",
    "eslint-plugin-import": "^2.31.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0",
    "vitest": "^2.1.5"
  },
  "dependencies": {
    "eslint": "9.10.0"
  },
  "volta": {
    "node": "20.11.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "aws",
    "cdk"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ren-yamanashi/eslint-plugin-cdk.git"
  },
  "author": {
    "name": "ren-yamanashi"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
